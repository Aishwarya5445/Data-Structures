#include <stdio.h> 
void printArray(int arr[], int size) 
 { 
    printf("Array elements are: ");  
    for(int i = 0; i < size; i++) 
   { 
        printf("%d ", arr[i]); 
    } 
    printf("\n"); 
 } 
int sumArray(int arr[], int size) 
 { 
    int sum = 0;    
    for(int i = 0; i < size; i++) 
    { 
        sum += arr[i]; 
     } 
    return sum; 
 } 
int main() 
 { 
    int arr[5] = {10, 20, 30, 40, 50}; 
    int size = 5; 
    printArray(arr, size);  
    int sum = sumArray(arr, size);   
    printf("Sum of array elements = %d\n", sum);   
    return 0; 
} 
