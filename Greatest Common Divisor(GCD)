#include<stdio.h>
int gcd(int a,int b) 
{ 
	 	if(a==0) 
	 	 	return b; 
	 	else if(b==0) 
	 	 	return a; 
	 	else if(a<b) 
 	 	  return gcd(a,b%a);  
    else  	 
      return gcd(b,a%b); 	 
} 
void main() 
{ 
 	int x,y,GCD; 
  printf("Enter two integers: ");  
  scanf("%d%d",&x,&y);  	
  GCD=gcd(x,y);  	
  printf("GCD of %d and %d is %d\n",x,y, GCD); 
} 
