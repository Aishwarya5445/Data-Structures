#include<stdio.h>
#include<stdlib.h>
struct node
{
int data;
struct node *next;
struct node *prev;
};
struct node *head,*temp,*new,*last=NULL;
struct node *getnode(int x)
{
struct node *temp=(struct node*)malloc(sizeof(struct node));
temp->data=x;
temp->next=NULL;

Data Structures Lab Roll Number: 25335A0515

82 | P a g e Dept. of CSE, MVGR College of Engineering (Autonomous)
temp->prev=NULL;
return temp;
}
void insert_begin(int x)
{
new=getnode(x);
if(head==NULL)
head=last=new;
else
{
new->next=head;
head->prev=new;
head=new;
}
}
void display()
{
if(head==NULL)
{
printf("list empty\n");
}
else
{
temp=head;
while(temp!=NULL)
{
printf("%d->",temp->data);
temp=temp->next;

