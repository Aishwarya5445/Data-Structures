#include<stdio.h>
#include <stdlib.h>
#include<ctype.h>
int s[50];
int top=-1;
void push(int elem)
{
  s[++top]=elem;
}
int pop()
{
  return(s[top--]);
}
void main()
{
char pofx[50],ch;
int i=0,op1,op2;
printf("\n\nEnter the Postfix Expression ");
scanf("%s",pofx);
while( (ch=pofx[i++]) !='\0')
{
if(isdigit(ch))
push(ch-'0');
else
{
op2=pop();
op1=pop();
switch(ch)
{
case '+':push(op1+op2);
         break;
case '-':push(op1-op2);
         break;
case '*':push(op1*op2);
         break;
case '/':push(op1/op2);
         break;
}
}
}
printf(" Given Postfix Expn: %s\n",pofx);
printf(" Result after Evaluation: %d\n",s[top]);
}
