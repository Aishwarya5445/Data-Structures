#include <stdio.h> 
int vt[10], dfs[10], top = -1; 
void push(int n) 
 { 
    dfs[++top] = n; 
} 
void pop()  
{ 
    top--; 
} 
void dfs_Traversal(int a[][10], int n, int start)  
{ 
    int i, found; 
    for (i = 0; i < n; i++) 
        vt[i] = 0;  
    start = start - 1;   
    push(start); 
    vt[start] = 1; 
    printf("DFS visited nodes are: %d", start + 1); 
    while (top != -1)  
{ 
        start = dfs[top]; 
        found = 0;   
        for (i = 0; i < n; i++) { 
            if (a[start][i] == 1 && vt[i] == 0) { 
                push(i); 
                printf(" -> %d", i + 1); 
                vt[i] = 1; 
                found = 1; 
                break; 
            } 
        } 
        if (found == 0)  
            pop(); 
    } 
} 
int main()  
{ 
    int a[10][10], n, i, j, id; 
    printf("Enter number of vertices in graph: "); 
    scanf("%d", &n); 
    printf("Enter adjacency matrix:\n"); 
    for (i = 0; i < n; i++) 
        for (j = 0; j < n; j++) 
            scanf("%d", &a[i][j]); 
    printf("Enter starting vertex: "); 
    scanf("%d", &id); 
    dfs_Traversal(a, n, id); 
    return 0; 
}
