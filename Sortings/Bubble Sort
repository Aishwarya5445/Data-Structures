#include<stdio.h> 
int main() 
{ 
int i,j,n,a[100],temp;
printf("enter the number of elements");
scanf("%d",&n);
printf("enter %d elements\n",n);
for (i=0;i<n;i++) 
{ 
scanf("%d",&a[i]); 
} 
for (i=0;i<n;i++) 
{ 
for (j=0;j<n-1;j++) 
{ 
if (a[j]>a[j+1]) 
{ 
temp= a[j]; 
a[j]=a[j+1]; 
1 
a[j+1]=temp; 
} 
} 
} 
printf("elements after sorting:\n"); 
for (i=0;i<n;i++) 
{ 
printf("%d\n",a[i]); 
} 
return 0; 
}
